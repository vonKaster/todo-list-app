{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport HelloWorld from \"../components/HelloWorld\";\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      listaTareas: [{\n        id: 1,\n        titulo: 'Título Tarea 1',\n        descripcion: 'dsfgfsdgfdsgfdgfdfdgfd'\n      }],\n      titulo: '',\n      descripcion: '',\n      multiLine: true,\n      snackbar: false,\n      mensaje: '',\n      formAgregar: true\n    };\n  },\n  components: {\n    HelloWorld\n  },\n  methods: {\n    agregarTarea() {\n      if (this.titulo === '' || this.descripcion === '') {\n        this.snackbar = true;\n        this.mensaje = '¡Debes llenar todos los campos!';\n      } else {\n        this.listaTareas.push({\n          id: Date.now(),\n          titulo: this.titulo,\n          descripcion: this.descripcion\n        });\n        this.titulo = '', this.descripcion = '';\n        this.snackbar = true;\n        this.mensaje = 'Tarea agregada';\n      }\n    },\n    eliminarTarea(id) {\n      this.listaTareas = this.listaTareas.filter(elemento => elemento.id != id);\n    }\n  }\n};","map":{"version":3,"mappings":";AA4DA;AAEA;EACAA;EAEAC;IACA;MACAC;QACAC;QAAAC;QAAAC;MACA;MACAD;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EAEAC;IACAC;EACA;EAEAC;IAEAC;MACA;QACA;QACA;MACA;QACA;UACAV;UACAC;UACAC;QACA;QACA,kBACA;QACA;QACA;MACA;IACA;IACAS;MACA;IACA;EAEA;AACA","names":["name","data","listaTareas","id","titulo","descripcion","multiLine","snackbar","mensaje","formAgregar","components","HelloWorld","methods","agregarTarea","eliminarTarea"],"sourceRoot":"src/views","sources":["HomeView.vue"],"sourcesContent":["<template>\n  <v-container grid-list-xl>\n    <v-layout row wrap>\n      <v-flex md6>\n        <v-card v-for=\"(tarea, index) in listaTareas\" :key=\"index\" class=\"pa-md-4 mb-4\">\n          <v-chip class=\"ma-2 ml-0\" color=\"indigo\" label text-color=\"white\">\n            <v-icon left> mdi-label </v-icon>\n            {{tarea.titulo}}\n          </v-chip>\n          <p>\n            {{tarea.descripcion}}\n          </p>\n          <v-btn color=\"warning\">Editar</v-btn>\n          <v-btn @click=\"eliminarTarea(tarea.id)\" color=\"error\" class=\"ms-3\">Eliminar</v-btn>\n        </v-card>\n      </v-flex>\n      <v-flex md6 v-if=\"formAgregar\">\n        <v-card class=\"mb-3 pa-3\">\n          <v-form @submit.prevent=\"agregarTarea\">\n            <v-text-field v-model=\"titulo\" label=\"Título de la tarea\"></v-text-field>\n            <v-textarea v-model=\"descripcion\" label=\"Descripción de la tarea\"></v-textarea>\n            <v-btn block color=\"success\" type=\"submit\">Agregar Tarea</v-btn>\n          </v-form>\n        </v-card>\n      </v-flex>\n\n      <v-flex md6 v-if=\"!formAgregar\">\n        <v-card class=\"mb-3 pa-3\">\n          <v-form @submit.prevent=\"agregarTarea\">\n            <v-text-field v-model=\"titulo\" label=\"Título de la tarea\"></v-text-field>\n            <v-textarea v-model=\"descripcion\" label=\"Descripción de la tarea\"></v-textarea>\n            <v-btn block color=\"warning\" type=\"submit\">Editar Tarea</v-btn>\n          </v-form>\n        </v-card>\n      </v-flex>\n\n    </v-layout>\n    \n    <v-snackbar\n      v-model=\"snackbar\"\n      :multi-line=\"multiLine\"\n    >\n      {{ mensaje }}\n\n      <template v-slot:action=\"{ attrs }\">\n        <v-btn\n          color=\"red\"\n          text\n          v-bind=\"attrs\"\n          @click=\"snackbar = false\"\n        >\n          Cerrar\n        </v-btn>\n      </template>\n    </v-snackbar>\n\n  </v-container>\n</template>\n\n<script>\nimport HelloWorld from \"../components/HelloWorld\";\n\nexport default {\n  name: \"Home\",\n\n  data(){\n    return {\n      listaTareas: [{\n        id: 1, titulo: 'Título Tarea 1', descripcion: 'dsfgfsdgfdsgfdgfdfdgfd'\n      }],\n      titulo: '',\n      descripcion: '',\n      multiLine: true,\n      snackbar: false,\n      mensaje: '',\n      formAgregar: true\n    }\n  },\n\n  components: {\n    HelloWorld,\n  },\n\n  methods: {\n\n    agregarTarea() {\n      if(this.titulo === '' || this.descripcion === ''){\n        this.snackbar = true\n        this.mensaje = '¡Debes llenar todos los campos!'\n      } else {\n        this.listaTareas.push({\n          id: Date.now(),\n          titulo: this.titulo,\n          descripcion: this.descripcion\n        })\n        this.titulo = '',\n        this.descripcion = ''\n        this.snackbar = true\n        this.mensaje = 'Tarea agregada'\n      }\n    },\n    eliminarTarea(id) {\n      this.listaTareas = this.listaTareas.filter(elemento => elemento.id != id)\n    }\n\n  }\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}